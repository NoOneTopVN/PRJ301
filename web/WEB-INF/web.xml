    <servlet>
        <servlet-name>DetailServlet</servlet-name>
        <servlet-class>nghiaht.servlet.DetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>nghiaht.servlet.SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>nghiaht.servlet.SignUpServlet</servlet-class>
